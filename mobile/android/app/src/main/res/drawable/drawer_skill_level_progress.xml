<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="@dimen/dimen_20"/>
            <solid android:color="@color/drawer_skill_progress_default" />
            <stroke android:color="@color/drawer_image_outline" android:width="@dimen/dimen_1" ></stroke>
        </shape>
    </item>

    <item  android:id="@android:id/progress">
        <clip>
            <shape xmlns:android="http://schemas.android.com/apk/res/android">
                <corners android:radius="@dimen/dimen_20"/>
                <solid android:color="@color/drawer_skill_progress_indicator" />
                <stroke android:color="@color/drawer_image_outline" android:width="@dimen/dimen_1" ></stroke>
            </shape>
        </clip>
    </item>
</layer-list>
